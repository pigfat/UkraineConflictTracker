{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
    "AllowedHosts": "*",
    "ConnectionStrings": {
        "Default": "host=#{dbHost}#;Port=#{dbPort}#;Database=#{dbName}#;Uid=#{dbUser}#;Pwd=#{dbPassword}#;Ssl-Mode=REQUIRED;"
    }
}
